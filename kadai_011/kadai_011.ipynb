        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "# for文での要素の取り出し\n",
        "for i in range(len(array)):\n",
        "  print(array[i])\n",
        "\n",
        "# while文での要素の取り出し\n",
        "i = 0\n",
        "while(i < len(array)):\n",
        "  print(array[i])\n",
        "  i = i + 1"
